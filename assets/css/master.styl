@import '_settings'
@import '_insta'

normalize-css()
base()

.logobox
 
  .logo 
    text-align: center
    font-size: 20px    

  .links
    text-align: center
    margin-bottom: 15px
    font-size: 15px
    a
      color: #35404C
      text-decoration: none

body
  padding: 10px
  height: 100%

.photo
  display: block
  +below(780px)
    margin: 0px 0px 0px 0px

  img
    display: block
    margin: 0 auto
    max-width: 100%
    max-height: 100%

.caption
  text-align: center

